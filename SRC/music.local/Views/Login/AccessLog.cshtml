@using System.Data

@{
    Layout = null;
    var data = ViewBag.ListAccessLog as DataTable;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    <link href="~/Content/bootstrap3.3.7/css/bootstrap.css" rel="stylesheet" />
    <script src="~/Content/JS/jquery-3.1.1.min.js"></script>
</head>
<body>
    @if (data != null && data.Rows.Count > 0)
    {
        var count = 1;
        <table class="table table-striped table-hover  table-condensed">         
            <tr>
                <th>No</th>
                <th>IP</th>
                <th>Created</th>
                <th>Expired</th>
                <th>Infor</th>
                <th>Last Active</th>
            </tr>
            <tbody>
                <tr>
                    @foreach (DataRow item in data.Rows)
                    {
                        <td>@count</td>
                        <td>@item["Identity"]</td>
                        <td>@item["Created"]</td>
                        <td>@item["Expired"]</td>
                        <td>@item["OtherInfor"]</td>
                        <td>@item["LastActive"]</td>
                        count++;
                    }
                </tr>
            </tbody>
        </table>
    }
</body>
